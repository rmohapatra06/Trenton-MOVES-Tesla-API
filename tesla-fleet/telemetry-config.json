{
    "host": "0.0.0.0",
    "port": 443,
    "log_level": "info",
    "json_log_enable": true,
    "namespace": "tesla",
    "reliable_ack": false,
    "transmit_decoded_records": true,
    "records": { "V": ["logger"], "alerts": ["logger"], "errors": ["logger"] },
    "tls": {
      "server_cert": "/config/tls-cert.pem",
      "server_key": "/config/tls-key.pem"
    }
  }